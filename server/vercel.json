{
    "version": 2,
    "builds": [
      {
        "src": "/src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/openapi.json",
        "dest": "/public/openapi.json"
      },
      {
        "src": "/(.*)",
        "dest": "/src/index.js"
      }
    ]
  }
  